<div>
    <h2>Todos os Dados</h2>
    <table>
        <tr>
            <th>Id</th>
            <th>Nome</th>
            <th>Estado</th>
            <th>Habitantes</th>
        </tr>
        @{
            foreach(var item in Model){
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Nome</td>
                    <td>@item.Estado</td>
                    <td>@item.Habitantes</td>
                </tr>
            }
        }
    </table>
</div>